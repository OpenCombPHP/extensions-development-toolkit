<?xml version="1.0" encoding="utf-8"?>
<Extension>

	<name>{=$sExtName}</name>
	<version>{=$sExtVersion}</version>
	<title>{=$sExtTitle}</title>
	
<if $sClassNamespace>
	<class>{=$sClassNamespace}.{=$sClassName}</class>
	<package folder="/class" namespace="{=$sClassNamespace}" />
</if>
	
</Extension>
